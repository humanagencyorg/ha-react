{
  "name": "@humanagency/ha-react",
  "version": "1.0.16",
  "description": "Human Agency React components library",
  "author": "valdemarua",
  "license": "MIT",
  "repository": "humanagencyorg/ha-react",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "lint": "eslint src",
    "prepare": "run-s build",
    "test": "jest",
    "predeploy": "cd example && npm install && npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token ttjfex5dqp"
  },
  "peerDependencies": {
    "react": "^16.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.7.0",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/react": "^6.1.14",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "chromatic": "^5.6.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^6.1.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-styled-components": "^6.3.4",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.11.0",
    "react-dom": "^16.12.0"
  },
  "files": ["dist"],
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.12",
    "@material-ui/pickers": "^3.3.10",
    "date-fns": "^2.19.0",
    "react-feather": "^2.0.8",
    "react-spinners": "^0.8.3",
    "styled-components": "^4.4.1"
  },
  "jest": {
    "setupFilesAfterEnv": ["./src/setupTests.js"],
    "roots": ["./src"],
    "testRegex": "src/.*.test.js$",
    "moduleNameMapper": {
      "\\.(svg|png|jpg)$": "<rootDir>/src/__mocks__/file_mock.js"
    },
    "transformIgnorePatterns": ["node_modules/(?!react-feather/.*)"]
  },
  "browserslist": ["defaults"]
}
